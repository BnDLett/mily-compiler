/-------- Standard library file --------

raw void print(any x) $
    print x
$

raw void printflush(symbol target) $
    printflush target
$

raw void wait(int x) $
    wait x
$

raw void wait(double x) $
    wait x
$

raw void end() $
    end
$

raw void stop() $
    stop
$

raw double pow(any x, any y) -> output $
    op pow output x y
$

raw int as_int(double x) -> output $
    op floor output x
    jump output greaterThanEq output 0
        op add output output 1
    output:
$

