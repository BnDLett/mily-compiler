--------- Math Utils --------
-- TODO: create something to prevent copy pastes
raw double pow(double x, double y) -> output ${
    op pow output x y
}

raw double pow(int x, int y) -> output ${
    op pow output x y
}

raw double pow(double x, int y) -> output ${
    op pow output x y
}

raw double pow(int x, double y) -> output ${
    op pow output x y
}

raw int as_int(double x) -> output ${
    op floor output x
    jump output greaterThanEq output 0
        op add output output 1
    output:
}

raw void inc(int i) ${
    op add i i 1
}

raw void inc(double i) ${
    op add i i 1
}