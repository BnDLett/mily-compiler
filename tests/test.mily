#include std/bulb.mily;

int f(int x) {
    int out = 1;

    for (int i = 1; i <= x; i = i + 1) {
        out = out * i;
    }
    return out;
}

void main(int amount) {
    for (int i = 0; i < amount; i = i + 1) {
        print("factorial of ");
        print(i);
        print(": ");
        print(f(i));
        print("\n");
    }
    printflush(@message1);
}

main(16);

