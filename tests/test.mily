#include std/bulb.mily;

int f(int x) {
    int out = 1;

    for (int i = 1; i <= x; i = i + 1) {
        out = out * i;
    }
    return out;
}

print$("\n");
print$("\n");

for (int i = 0; i < 20; i = i + 1) {
    print$("factorial of ");
    print$(i);
    print$(": ");
    print$(f(i));
    print$("\n");
}
